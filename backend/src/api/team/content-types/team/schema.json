{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "alternate_names": {
      "type": "json",
      "default": []
    },
    "city": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "short_name": {
      "type": "string",
      "required": true,
      "regex": ".{3}$"
    },
    "country": {
      "type": "string"
    }
  }
}
